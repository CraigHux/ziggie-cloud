    "components": {
      "MuiCssBaseline": {
        "styleOverrides": {
          "body": {
            "scrollbarColor": "#6B7A90 #1A2332",
            "scrollbarWidth": "thin",
            "&::-webkit-scrollbar": {
              "width": "8px",
              "height": "8px"
            },
            "&::-webkit-scrollbar-track": {
              "background": "#1A2332"
            },
            "&::-webkit-scrollbar-thumb": {
              "background": "#6B7A90",
              "borderRadius": "4px"
            },
            "&::-webkit-scrollbar-thumb:hover": {
              "background": "#B2BAC2"
            }
          },
          "*:focus-visible": {
            "outline": "2px solid #3B82F6",
            "outlineOffset": "2px",
            "borderRadius": "4px"
          },
          "a:focus-visible, button:focus-visible, [role='button']:focus-visible": {
            "outline": "2px solid #3B82F6",
            "outlineOffset": "2px"
          }
        }
      },
      "MuiCard": {
        "styleOverrides": {
          "root": {
            "borderRadius": 12,
            "boxShadow": "0 4px 6px rgba(0, 0, 0, 0.3)",
            "transition": "box-shadow 200ms ease-in-out, transform 150ms ease-out",
            "&:hover": {
              "boxShadow": "0 10px 15px rgba(0, 0, 0, 0.25)",
              "transform": "translateY(-2px)"
            }
          }
        }
      },
      "MuiButton": {
        "styleOverrides": {
          "root": {
            "borderRadius": 8,
            "textTransform": "none",
            "fontWeight": 600,
            "fontSize": "0.875rem",
            "padding": "10px 20px",
            "boxShadow": "0 1px 3px rgba(0, 0, 0, 0.4)",
            "transition": "background-color 150ms ease-in-out, box-shadow 150ms ease-in-out, transform 100ms ease-out",
            "&:hover": {
              "boxShadow": "0 2px 4px rgba(0, 0, 0, 0.35)"
            },
            "&:active": {
              "transform": "scale(0.98)"
            },
            "&:focus-visible": {
              "outline": "2px solid #3B82F6",
              "outlineOffset": "2px",
              "boxShadow": "0 0 0 3px rgba(59, 130, 246, 0.3)"
            }
          },
          "containedPrimary": {
            "backgroundColor": "#FF8C42",
            "&:hover": {
              "backgroundColor": "#CC6600"
            }
          },
          "containedSecondary": {
            "backgroundColor": "#4169E1",
            "&:hover": {
              "backgroundColor": "#2952CC"
            }
          },
          "outlined": {
            "borderWidth": "1px",
            "borderColor": "#FF8C42",
            "color": "#FF8C42",
            "&:hover": {
              "borderColor": "#CC6600",
              "backgroundColor": "rgba(255, 140, 66, 0.08)"
            }
          }
        }
      },
      "MuiIconButton": {
        "styleOverrides": {
          "root": {
            "borderRadius": 8,
            "transition": "background-color 150ms ease-in-out, color 150ms ease-in-out, transform 150ms ease-out",
            "&:hover": {
              "backgroundColor": "rgba(231, 235, 240, 0.08)",
              "transform": "scale(1.1)"
            },
            "&:focus-visible": {
              "outline": "2px solid #3B82F6",
              "outlineOffset": "2px",
              "boxShadow": "0 0 0 3px rgba(59, 130, 246, 0.3)"
            }
          }
        }
      },
      "MuiChip": {
        "styleOverrides": {
          "root": {
            "borderRadius": 12,
            "fontWeight": 600,
            "fontSize": "0.75rem"
          },
          "filled": {
            "backgroundColor": "#243447"
          },
          "filledSuccess": {
            "backgroundColor": "#10B981",
            "color": "#FFFFFF"
          },
          "filledWarning": {
            "backgroundColor": "#F59E0B",
            "color": "#FFFFFF"
          },
          "filledError": {
            "backgroundColor": "#EF4444",
            "color": "#FFFFFF"
          },
          "filledInfo": {
            "backgroundColor": "#3B82F6",
            "color": "#FFFFFF"
          }
        }
      },
      "MuiTextField": {
        "styleOverrides": {
          "root": {
            "& .MuiOutlinedInput-root": {
              "borderRadius": 8,
              "backgroundColor": "#243447",
              "transition": "border-color 200ms ease-in-out, box-shadow 200ms ease-in-out",
              "& fieldset": {
                "borderColor": "#2D3F52"
              },
              "&:hover fieldset": {
                "borderColor": "#6B7A90"
              },
              "&.Mui-focused fieldset": {
                "borderColor": "#4169E1",
                "boxShadow": "0 0 0 3px rgba(65, 105, 225, 0.1)"
              }
            }
          }
        }
      },
      "MuiPaper": {
        "styleOverrides": {
          "root": {
            "backgroundImage": "none"
          },
          "elevation1": {
            "boxShadow": "0 1px 3px rgba(0, 0, 0, 0.4)"
          },
          "elevation2": {
            "boxShadow": "0 2px 4px rgba(0, 0, 0, 0.35)"
          },
          "elevation3": {
            "boxShadow": "0 4px 6px rgba(0, 0, 0, 0.3)"
          },
          "elevation4": {
            "boxShadow": "0 10px 15px rgba(0, 0, 0, 0.25)"
          }
        }
      },
      "MuiAppBar": {
        "styleOverrides": {
          "root": {
            "boxShadow": "0 1px 3px rgba(0, 0, 0, 0.4)",
            "borderBottom": "1px solid #2D3F52"
          }
        }
      },
      "MuiDrawer": {
        "styleOverrides": {
          "paper": {
            "backgroundColor": "#1A2332",
            "borderRight": "1px solid #2D3F52"
          }
        }
      },
      "MuiListItemButton": {
        "styleOverrides": {
          "root": {
            "borderRadius": 8,
            "margin": "4px 8px",
            "transition": "background-color 150ms ease-in-out, color 150ms ease-in-out",
            "&:hover": {
              "backgroundColor": "#243447"
            },
            "&.Mui-selected": {
              "backgroundColor": "rgba(255, 140, 66, 0.16)",
              "borderLeft": "3px solid #FF8C42",
              "color": "#FF8C42",
              "&:hover": {
                "backgroundColor": "rgba(255, 140, 66, 0.24)"
              }
            }
          }
        }
      },
      "MuiTooltip": {
        "styleOverrides": {
          "tooltip": {
            "backgroundColor": "#243447",
            "fontSize": "0.75rem",
            "borderRadius": 6,
            "boxShadow": "0 4px 6px rgba(0, 0, 0, 0.3)"
          }
        }
      },
      "MuiTableCell": {
        "styleOverrides": {
          "root": {
            "borderBottom": "1px solid #2D3F52"
          },
          "head": {
            "fontWeight": 600,
            "backgroundColor": "#243447",
            "color": "#E7EBF0"
          }
        }
      },
      "MuiDivider": {
        "styleOverrides": {
          "root": {
            "borderColor": "#2D3F52"
          }
        }
      },
      "MuiAlert": {
        "styleOverrides": {
          "root": {
            "borderRadius": 8
          },
          "standardSuccess": {
            "backgroundColor": "#10B981",
            "color": "#FFFFFF"
          },
          "standardWarning": {
            "backgroundColor": "#F59E0B",
            "color": "#FFFFFF"
          },
          "standardError": {
            "backgroundColor": "#EF4444",
            "color": "#FFFFFF"
          },
          "standardInfo": {
            "backgroundColor": "#3B82F6",
            "color": "#FFFFFF"
          }
        }
      },
      "MuiLinearProgress": {
        "styleOverrides": {
          "root": {
            "borderRadius": 2,
            "height": 4,
            "backgroundColor": "#2D3F52"
          },
          "barColorPrimary": {
            "backgroundColor": "#FF8C42"
          }
        }
      },
      "MuiCircularProgress": {
        "styleOverrides": {
          "colorPrimary": {
            "color": "#FF8C42"
          }
        }
      }
    }
  },

